using namespace std;







int tong(int a);



int tong(int a, int b)
{
	 a=rand(0,100);
	b=rand(0,100);
	return a-b;
}
int main()
{
	int a;
	int b;
	cout << "DAY LA CHUONG TRINH DE THUC HANH GIT";
	cout << "\nTong 2 so " << a << " va " << b << " la: " << tong(a,b);


	

	system("pause");
	return 0;
}